.container {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #f4f4f4;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.title {
  font-size: 40rpx;
}

.button-container {
  font-size: 32rpx;
  font-weight: bold;
  margin-top: 50rpx;
}

.button {
  background: #ffd161;
}
